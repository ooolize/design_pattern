Checks: >
  clang-analyzer-*,
  modernize-*,
  cppcoreguidelines-*,
  readability-*,
  performance-*
WarningsAsErrors: ''
HeaderFilterRegex: ''
AnalyzeTemporaryDtors: false
FormatStyle: none
User: ''
CheckOptions:
  - key:             modernize-use-nullptr.NullMacros
    value:           'NULL'
  - key:             readability-identifier-naming.VariableCase
    value:           'lower_case'
